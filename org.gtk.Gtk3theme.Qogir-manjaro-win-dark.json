{
  "id": "org.gtk.Gtk3theme.Qogir-manjaro-win-dark",
  "branch": "3.22",
  "runtime": "org.gnome.Sdk",
  "build-extension": true,
  "sdk": "org.gnome.Sdk",
  "runtime-version": "3.26",
  "appstream-compose": false,
  "separate-locales": false,
  "modules": [
    {
      "name": "Qogir-manjaro-win-dark",
      "buildsystem": "simple",
      "build-commands": [
        "mkdir -pm 755 /usr/share/runtime/share/themes/Qogir-manjaro-win-dark/gtk-3.0",
        "cp src/gtk-3.0/gtk-win-dark.css /usr/share/runtime/share/themes/Qogir-manjaro-win-dark/gtk-3.0/gtk.css",
        "cp -r src/gtk-3.0/assets /usr/share/runtime/share/themes/Qogir-manjaro-win-dark/gtk-3.0"
      ],
      "sources": [
        {
          "type": "archive",
          "url": "https://github.com/vinceliuice/Qogir-theme/archive/Qogir-theme-2019-10-25.tar.gz",
          "sha256": "07027ad930f4cba10c92ce782fa6b3747afefaafc4bc029692e95ad8f3dcd1ff"
        }
      ]
    },
    {
      "name": "appdata",
      "buildsystem": "simple",
      "build-commands": [
        "mkdir -p ${FLATPAK_DEST}/share/appdata",
        "cp org.gtk.Gtk3theme.Qogir-manjaro-win-dark.appdata.xml ${FLATPAK_DEST}/share/appdata",
        "appstream-compose --basename=org.gtk.Gtk3theme.Qogir-manjaro-win-dark --prefix=${FLATPAK_DEST} --origin=flatpak org.gtk.Gtk3theme.Qogir-manjaro-win-dark"
      ],
      "sources": [
        {
          "type": "file",
          "path": "org.gtk.Gtk3theme.Qogir-manjaro-win-dark.appdata.xml"
        }
      ]
    }
  ]
}
