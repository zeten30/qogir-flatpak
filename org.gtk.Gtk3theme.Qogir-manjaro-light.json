{
  "id": "org.gtk.Gtk3theme.Qogir-manjaro-light",
  "branch": "3.34",
  "runtime": "org.gnome.Sdk",
  "build-extension": true,
  "sdk": "org.gnome.Sdk",
  "runtime-version": "3.34",
  "appstream-compose": false,
  "separate-locales": false,
  "modules": [
    {
      "name": "Qogir-manjaro-light",
      "buildsystem": "simple",
      "build-commands": [
        "mkdir -pm 755 /usr/share/runtime/share/themes/Qogir-manjaro-light/gtk-3.0/assets",
        "cp -fr src/gtk-3.0/assets/assets-common/* /usr/share/runtime/share/themes/Qogir-manjaro-light/gtk-3.0/assets/",
        "cp -fr src/gtk-3.0/assets/assets-manjaro/* /usr/share/runtime/share/themes/Qogir-manjaro-light/gtk-3.0/assets/",
        "cp src/gtk-3.0/theme/gtk-dark.css /usr/share/runtime/share/themes/Qogir-manjaro-light/gtk-3.0/gtk-dark.css",
        "cp src/gtk-3.0/theme/gtk-light.css /usr/share/runtime/share/themes/Qogir-manjaro-light/gtk-3.0/gtk.css"
      ],
      "sources": [
        {
          "type": "archive",
          "url": "https://github.com/vinceliuice/Qogir-theme/archive/2020-02-26.tar.gz",
          "sha256": "0cce185bf300ef2265ba435d3e9e255506ea9093a24d0f0ee57097a8b52320fa"
        }
      ]
    },
    {
      "name": "appdata",
      "buildsystem": "simple",
      "build-commands": [
        "mkdir -p ${FLATPAK_DEST}/share/appdata",
        "cp org.gtk.Gtk3theme.Qogir-manjaro-light.appdata.xml ${FLATPAK_DEST}/share/appdata",
        "appstream-compose --basename=org.gtk.Gtk3theme.Qogir-manjaro-light --prefix=${FLATPAK_DEST} --origin=flatpak org.gtk.Gtk3theme.Qogir-manjaro-light"
      ],
      "sources": [
        {
          "type": "file",
          "path": "org.gtk.Gtk3theme.Qogir-manjaro-light.appdata.xml"
        }
      ]
    }
  ]
}
